---
import Layout from '../../../../layouts/MainLayout.astro';
import ProductLayout from '../../../../layouts/ProductLayout.astro';
import ImageCarousel from '../../../../components/ImageCarousel';
import OrderCTA from '../../../../components/OrderCTA.astro';

const slides = [
  {
    image: '/images/constructor_denmark_multiline_1_large.webp',
  },
  {
    image: '/images/constructor_denmark_multiline_2_large.webp',
  },
  {
    image: '/images/constructor_denmark_multiline_3_large.webp',
  },
  {
    image: '/images/constructor_denmark_multiline_4_large.webp',
  },
];

const sidebarItems = [
  {
    title: 'CePT Юкей ПиЛСи, Великобритания',
