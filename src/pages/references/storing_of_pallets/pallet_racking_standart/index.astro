    href: '/references/storing_of_pallets/pallet_racking_standart/dastin_shvecija/',
  },
];
---

<Layout title="Реализованные проекты с применением фронтальных паллетных стеллажей">
  <Breadcrumbs items={breadcrumbItems} />

  <div class="max-w-6xl mx-auto px-4 py-8">
    <h1 class="text-white bg-gray-600 px-6 py-3 text-lg rounded-lg shadow mb-8">
      Реализованные проекты с применением фронтальных паллетных стеллажей
    </h1>

    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8">
      {
        projects.map((project) => (
          <ProjectCard
            href={project.href}
            title={project.title}
            description={project.description}
            image={project.image}
          />
        ))
      }
    </div>
  </div>
</Layout>
